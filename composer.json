{
    "name": "nrep/form-forge",
    "description": "Schema-driven form builder for PHP - Framework agnostic with Tailwind, Bootstrap, and Alpine.js support",
    "type": "library",
    "license": "MIT",
    "keywords": ["form", "builder", "schema", "php", "tailwind", "bootstrap", "alpine"],
    "authors": [
        {
            "name": "NREP",
            "email": "elvisrugero@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FormForge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FormForge\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
